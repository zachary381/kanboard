{
    "name": "kanboard/kanboard",
    "type": "project",
    "description": "Kanban project management software",
    "license": "MIT",
    "authors": [
        {
            "name": "Frédéric Guillot"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "discard-changes": true
    },
    "require" : {
        "php" : ">=7.2.0",
        "ext-simplexml" : "*",
        "ext-dom" : "*",
        "ext-xml" : "*",
        "ext-gd" : "*",
        "ext-mbstring" : "*",
        "ext-hash" : "*",
        "ext-openssl" : "*",
        "ext-json" : "*",
        "ext-ctype" : "*",
        "ext-filter" : "*",
        "ext-session" : "*",
        "christian-riesen/otp" : "2.7.0",
        "eluceo/ical": "0.16.1",
        "erusev/parsedown" : "^1.7",
        "pimple/pimple" : "3.4.0",
        "psr/log": "~1.0",
        "swiftmailer/swiftmailer" : "5.4.8",
        "symfony/console" : "4.2.12",
        "symfony/event-dispatcher" : "3.4.2",
        "gregwar/captcha": "1.1.9"
    },
    "autoload" : {
        "classmap" : ["app/"],
        "psr-4" : {
            "Kanboard\\" : "app/"
        },
        "psr-0" : {
            "": "libs/"
        },
        "files" : [
            "app/functions.php"
        ]
    },
    "require-dev" : {
        "symfony/stopwatch" : "5.3.4",
        "phpunit/phpunit" : "^7",
        "roave/security-advisories": "dev-master"
    }
}
